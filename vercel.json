{
  "crons": [
    {
      "path": "/api/cron/distribute-weekly-profits?auth_key=${CRON_SECRET}",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/cron/update-roi?auth_key=${CRON_SECRET}",
      "schedule": "0 0 * * 0"
    },
    {
      "path": "/api/cron/send-meet-links?auth_key=${CRON_SECRET}",
      "schedule": "* * * * *"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
} 